{
    "name": "timiki/rpc-client",
    "type": "library",
    "description": "Simple HTTP JSON-RPC client",
    "keywords": [
        "rpc",
        "json-rpc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Netesin",
            "email": "nikita@netesin.ru"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "timiki/rpc-common": "^1.0|^2.0",
        "guzzlehttp/guzzle": "^6.3|^7.1",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-symfony": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Timiki\\RpcClient\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}